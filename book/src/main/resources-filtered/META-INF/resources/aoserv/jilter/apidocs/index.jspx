    <ao:redirect page="index.html" statusCode="301" addLastModified="false" />
  </c:if>
  <core:page
    title="@{documented.name} ${ao:getProjectVersion('./', '@{project.groupId}', '@{documented.artifactId}', 'Â¿@{project.version}?')} API Docs"
    shortTitle="API Docs"
    datePublished="2020-04-13T12:04:48-05:00"
    dateModified="${ao:getLastModified('index.html')}"
    allowRobots="false"
  />
</jsp:root>
